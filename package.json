{
  "name": "artpulse-demo",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.15.9",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pretypecheck": "prisma generate",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "node --import tsx --test",
    "smoke": "node --test test/smoke.test.js && node -e \"fetch('http://localhost:3000/api/health').then(r=>r.json()).then(console.log).catch(()=>process.exit(0))\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "postinstall": "prisma generate",
    "prisma:check-migration-order": "node scripts/check-migration-order.mjs",
    "check-env": "node scripts/check-env.mjs",
    "perf:smoke": "node scripts/perf-smoke.mjs",
    "vercel:build": "node scripts/check-env.mjs && AUTH_SECRET=${AUTH_SECRET:-dev-secret} next build",
    "vercel:install": "pnpm install --frozen-lockfile"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.20",
    "@fullcalendar/daygrid": "^6.1.20",
    "@fullcalendar/list": "^6.1.20",
    "@fullcalendar/react": "^6.1.20",
    "@fullcalendar/timegrid": "^6.1.20",
    "@prisma/client": "^6.3.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@sentry/nextjs": "^10.39.0",
    "@vercel/blob": "^0.27.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.475.0",
    "mapbox-gl": "^3.18.1",
    "next": "15.5.12",
    "next-auth": "^4.24.13",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "tailwind-merge": "^2.6.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "eslint": "^9.19.0",
    "eslint-config-next": "15.5.12",
    "postcss": "^8.5.1",
    "prisma": "^6.3.1",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "typescript": "^5.7.3"
  }
}
